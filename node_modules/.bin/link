#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules/link/dist/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules/link/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules/link/dist/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules/link/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/link@2.1.1/node_modules:/Users/camimorales/Desktop/next-js-tut/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../link/dist/cli.js" "$@"
else
  exec node  "$basedir/../link/dist/cli.js" "$@"
fi
